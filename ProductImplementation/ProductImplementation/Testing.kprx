	public partial class DestinationRule_87bae239188b402099c856bc960aef54 : IWorkflowContext<hostContext>
	{
		#region K2 Context

        private hostContext _k2;

        #endregion

		#region IWorkflowContext<hostContext> Members

        public Project_cfaba09f17514e679ef0f5c9b5a89e22.DestinationRuleContext_87bae239188b402099c856bc960aef54 K2
        {
            get
