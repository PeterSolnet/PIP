using hostContext = Project_cb75c4449265451bb9a323824ac467bf.EventItemContext_71d4e4d68610422e9ca0bf49783f079f;

namespace ExtenderProject_cb75c4449265451bb9a323824ac467bf
{
	public partial class EventItem_71d4e4d68610422e9ca0bf49783f079f : IWorkflowContext<hostContext>
	{

		#region K2 Context

        private hostContext _k2;

        #endregion

        #region IWorkflowContext<hostContext> Members

        public Project_cb75c4449265451bb9a323824ac467bf.EventItemContext_71d4e4d68610422e9ca0bf49783f079f K2
        {
            get
            {
                return _k2;
            }
            set
            {
                _k2 = value;
            }
        }

        #endregion

        #region Workflow Activities

        private void IPCEvent_ExecuteCode(object sender, EventArgs e)
        {

            SCConnectionStringBuilder connectionBuilder = null;

            //ServerName
            if ((K2.Configuration.Server.IndexOf(":") < 0) && (K2.Configuration.Server.IndexOf("=") < 0))
            {
                connectionBuilder = new SCConnectionStringBuilder();
            }
            //ServerName:Port
            else if ((K2.Configuration.Server.IndexOf(":") > 0) && (K2.Configuration.Server.IndexOf("[") < 0))
            {
                connectionBuilder = new SCConnectionStringBuilder();
            }
            //[;]ServerName=;Port=..
            else if ((K2.Configuration.Server.IndexOf(":") > 0) && (K2.Configuration.Server.IndexOf("[") > 0))
            {
                connectionBuilder = new SCConnectionStringBuilder(K2.Configuration.Server);
            }
            //Integrate=True;..
            else if ((K2.Configuration.Server.Length != 0) && (K2.Configuration.Server.IndexOf("=") > 0))
            {
                connectionBuilder = new SCConnectionStringBuilder(K2.Configuration.Server);
            }
            else
            {
                connectionBuilder = new SCConnectionStringBuilder();
            }

            if (string.IsNullOrEmpty(connectionBuilder.Host))
            {
                //SERVER
                if (string.IsNullOrEmpty(K2.Configuration.Server))
                {
                    connectionBuilder.Host = "local";
                    connectionBuilder.Port = uint.Parse("5252");
                }
                else if (K2.Configuration.Server.IndexOf(":") < 0)
                {
                    connectionBuilder.Host = K2.Configuration.Server;
                    connectionBuilder.Port = uint.Parse("5252");
                }
                else if (K2.Configuration.Server.IndexOf(":") > 0)
                {
                    connectionBuilder.Host = K2.Configuration.Server.Split(":".ToCharArray())[0];
                    connectionBuilder.Port = uint.Parse(K2.Configuration.Server.Split(":".ToCharArray())[1]);
                }
            }

            //INFO
            if (!string.IsNullOrEmpty(K2.Configuration.UseIntegrated) && bool.Parse(K2.Configuration.UseIntegrated) == true)
            {
                connectionBuilder.Integrated = true;
            }

            //USER
            if (!string.IsNullOrEmpty(K2.Configuration.UseClearText) && bool.Parse(K2.Configuration.UseClearText) == true)
            {
                connectionBuilder.Integrated = true;

                if (!string.IsNullOrEmpty(K2.Configuration.SecurityLabel))
                {
                    connectionBuilder.SecurityLabelName = K2.Configuration.SecurityLabel;
                }
                if (!string.IsNullOrEmpty(K2.Configuration.UserName))
                {
                    connectionBuilder.UserID = K2.Configuration.UserName;
                }
                if (!string.IsNullOrEmpty(K2.Configuration.Password))
                {
                    connectionBuilder.Password = K2.Configuration.Password;
                }
                if (!string.IsNullOrEmpty(K2.Configuration.Domain))
                {
                    connectionBuilder.WindowsDomain = K2.Configuration.Domain;
                }
            }

            if (!string.IsNullOrEmpty(K2.Configuration.Anonymous) && bool.Parse(K2.Configuration.Anonymous) == true)
            {
                connectionBuilder.Integrated = false;
                connectionBuilder.AuthData = "Anonymous";
            }

            if (!string.IsNullOrEmpty(K2.Configuration.Originator) && bool.Parse(K2.Configuration.Originator) == true)
            {
                connectionBuilder.Integrated = true;
                connectionBuilder.AuthData = "Originator";
            }

            K2.Server = connectionBuilder.Host;

            K2.Port = int.Parse(connectionBuilder.Port.ToString());

            K2.ConnectionString = connectionBuilder.ConnectionString;

            // set destination process info
            K2.Process = K2.Configuration.Process;
            K2.Synchronous = bool.Parse(K2.Configuration.Synchronous);

            // bind destination process folio
            K2.GetServerContext().ActivityInstanceDestination.DataFields[K2.Configuration.FolioFieldName].Value = K2.Configuration.Folio;
            K2.BindFolio(IPCBindType.ActData, K2.Configuration.FolioFieldName);

            // bind source process fields to destination process fields
            BindFields();
        }
        
        #endregion

        #region Helper Methods

        private void BindFields()
        {
            System.Collections.Generic.List<IPCFieldMapping> list = new System.Collections.Generic.List<IPCFieldMapping>();

            string[][] mappings = K2.Configuration.FieldMappings;

            for (int i = 0; i < mappings.Length; i++)
            {
                try
                {
                    list.Add(new IPCFieldMapping(mappings[i][0], mappings[i][1], mappings[i][2], mappings[i][3], mappings[i][4]));
                }
                catch (Exception Ex)
                {
                    throw new Exception(Ex.Message);
                }
            }

            foreach (IPCFieldMapping item in list)
            {
                if (item.DataDirection == DataDirections.Out)
                {
                    try
                    {
                        K2.ActivityInstanceDestination.DataFields[item.SourceFieldName].Value = item.DestinationFieldValue;
                    }
                    catch (Exception Ex)
                    {
                        throw new Exception(Ex.Message);
                    }
                }

                if (item.DataDirection == DataDirections.Out)
                {
                    try
                    {
                        K2.BindValue(GetBindingType(item.SourceFieldType), item.DestinationFieldName, item.DestinationFieldValue, GetDataType(item.DestinationFieldValue));
                    }
                    catch (Exception Ex)
                    {
                        throw new Exception(Ex.Message);
                    }
                }
                else if ((item.DataDirection == DataDirections.In) || (item.DataDirection == DataDirections.InOut))
                {
                    try
                    {
                        K2.BindField(GetBindingType(item.SourceFieldType), item.SourceFieldName, item.DestinationFieldName, GetDirectionType(item.DataDirection));
                    }
                    catch (Exception Ex)
                    {
                        throw new Exception(Ex.Message);
                    }
                }
            }
        }

        private IPCBindDirection GetDirectionType(DataDirections value)
        {
            switch (value)
            {
                case DataDirections.In:
                    return IPCBindDirection.In;
                case DataDirections.InOut:
                    return IPCBindDirection.InOut;
                case DataDirections.Out:
                    return IPCBindDirection.Out;
                default:
                    return IPCBindDirection.InOut;
            }
        }

        private IPCBindType GetBindingType(BindFieldTypes value)
        {
            switch (value)
            {
                case SourceCode.Workflow.Common.IPC.BindFieldTypes.ActivityDataField:
                    return IPCBindType.ActData;
                case SourceCode.Workflow.Common.IPC.BindFieldTypes.ActivityXmlField:
                    return IPCBindType.ActXml;
                case SourceCode.Workflow.Common.IPC.BindFieldTypes.ProcessDataField:
                    return IPCBindType.ProcData;
                case SourceCode.Workflow.Common.IPC.BindFieldTypes.ProcessXmlField:
                    return IPCBindType.ProcXml;
                default:
                    return IPCBindType.ProcData;
            }
        }

        private DataType GetDataType(object value)
        {
            switch (Type.GetTypeCode(value.GetType()))
            {
                case TypeCode.Boolean:
                    return DataType.TypeBoolean;
                case TypeCode.Byte:
                    return DataType.TypeBinary;
                case TypeCode.DateTime:
                    return DataType.TypeDate;
                case TypeCode.Decimal:
                    return DataType.TypeDecimal;
                case TypeCode.Double:
                    return DataType.TypeDouble;
                case TypeCode.Int16:
                case TypeCode.Int32:
                case TypeCode.Int64:
                    return DataType.TypeInteger;
                case TypeCode.Single:
                    return DataType.TypeDouble;
                case TypeCode.String:
                    return DataType.TypeString;
                case TypeCode.UInt16:
                case TypeCode.UInt32:
                case TypeCode.UInt64:
                    return DataType.TypeLong;
                default:
                    return DataType.TypeString;
            }

        }

        #endregion

	}
}
