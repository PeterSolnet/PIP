            using(this.EmailMessage)
                SmtpMail.Send(this.EmailMessage, K2.ProcessInstance.Logger, System.Configuration.ConfigurationManager.ConnectionStrings["HostServer"].ConnectionString);

            this.EmailMessage = null;
        }

