                        // Binary 2 Base64
                        else
                        {
                            System.IO.MemoryStream ms = new System.IO.MemoryStream(System.Convert.FromBase64String(content));

                            this.EmailMessage.Attachments.Add(new Attachment(ms, filename));
                        }
                    }
                }
            }
