{
	public partial class DestinationRule_469c051dc2c4423b8a5e1b61ab5577ea : IWorkflowContext<hostContext>
	{
		#region K2 Context

        private hostContext _k2;

        #endregion

		#region IWorkflowContext<hostContext> Members

        public Project_f9f2883c9b7e4add998fa296fc00e9b8.DestinationRuleContext_469c051dc2c4423b8a5e1b61ab5577ea K2
        {
            get
