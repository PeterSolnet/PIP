		            // Invoke the get
		            System.Reflection.MethodInfo mi = pi.GetGetMethod();
		            if (mi == null)
		                return true;

		            object retval = mi.Invoke(K2.Configuration, null);

		            return Convert.ToBoolean(retval);
		        }
		}
	}
