	public partial class DestinationRule_f1f6c7665b664331829c03020de75682 : IWorkflowContext<hostContext>
	{
		#region K2 Context

        private hostContext _k2;

        #endregion

		#region IWorkflowContext<hostContext> Members

        public Project_f9f2883c9b7e4add998fa296fc00e9b8.DestinationRuleContext_f1f6c7665b664331829c03020de75682 K2
        {
            get
            {
